<?xml version="1.0" encoding="UTF-8"?>
<!--
  This file contains some false positive bugs detected by findbugs. Their
  false positive nature has been analyzed individually and they have been
  put here to instruct findbugs it must ignore them.

  For more information on adding filters to this file, refer to:
  https://spotbugs.readthedocs.io/en/stable/filter.html
-->
<FindBugsFilter>
    <Bug pattern="SE_NO_SERIALVERSIONID"/>
    <Match>
        <Class name="~.*PipelineSharedLib.*"/>
    </Match>
    <Bug pattern="NM_CLASS_NAMING_CONVENTION"/>
    <Match>
        <Class name="~.*logy.*"/>
    </Match>
    <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD"/>
    <Match>
        <Class name="~.*JenkinsPluginCheck.*"/>
    </Match>
    <Bug pattern="RANGE_ARRAY_INDEX"/>
    <Match>
        <Class name="~.*FeatureException.groovy"/>
    </Match>
</FindBugsFilter>